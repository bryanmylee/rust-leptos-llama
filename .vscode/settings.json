{
  "[rust]": {
    "editor.defaultFormatter": "bram209.leptos-language-server"
  },
  "css.validate": false,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": "on"
  },
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html",
  },
  "files.associations": {
    "*.rs": "rust"
  },
  "rust-analyzer.cargo.features": "all",
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*.rs": "html",
  },
}